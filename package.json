{
    "name": "Troyblank.com",
    "version": "0.1.0",
    "main": "server.js",
    "scripts": {
        "start": "node server",
        "startnew": "npm run buildnew && cd out && serve -p 8888",
        "build": "grunt build",
        "buildnew": "next build && next export",
        "deploy": "grunt --environment production deploy",
        "test": "npm run unit && npm run lint",
        "unit": "nyc --reporter=text-summary --reporter=lcov --reporter=cobertura mocha --exit \"./src/**/*.spec.js\"",
        "lint": "npm run eslint && npm run sasslint",
        "eslint": "eslint './pages/**/*.js'",
        "sasslint": "sass-lint -c ./node_modules/@troyblank/sasslint-config-troyblank/.sass-lint.yml ./src/**/*.scss -v -q"
    },
    "nyc": {
        "temp-directory": "./coverage/.nyc_output"
    },
    "dependencies": {
        "next": "9.0.6",
        "react": "16.10.1",
        "react-dom": "16.10.1",
        "compression": "1.5.0",
        "connect-flash": "0.1.1",
        "body-parser": "1.12.4",
        "express": "4.12.3",
        "express-handlebars": "2.0.1",
        "morgan": "1.5.3"
    },
    "devDependencies": {
        "@babel/register": "7.6.2",
        "@troyblank/eslint-config-troyblank": "1.1.0",
        "@troyblank/mocha-base-troyblank": "1.0.0",
        "@troyblank/sasslint-config-troyblank": "1.0.0",
        "coveralls": "3.0.6",
        "eslint": "6.1.0",
        "sass-lint": "1.13.1",
        "serve": "11.2.0",
        "grunt": "0.4.5",
        "grunt-contrib-clean": "0.7.0",
        "grunt-contrib-uglify": "0.11.0",
        "grunt-ssh": "0.12.9",
        "load-grunt-config": "0.17.1"
    },
    "author": "Troy Blank",
    "license": "BSD-3-Clause"
}
